<% if(typeof errors != 'undefined') {%>
<% errors.forEach(function(error) { %>
    <div class="alert alert-warning aler-dismissable fade show" role="aler">
    <%= error.msg %>
    <button type="button" class="close" data-dismiss="alert" aria-label="close"><span aria-hidden="true">&times;</span>
    </button>
    </div>
<%}) %>
<% } %>

<% if(success_msg !=''){ %>
    <div class="alert alert-success aler-dismissable fade show" role="aler">
        <%= success_msg %>
        <button type="button" class="close" data-dismiss="alert" aria-label="close"><span aria-hidden="true">&times;</span>
        </button>
        </div>
    <%}) %>
<%}%>

<% if(error_msg !=''){ %>
    <div class="alert alert-warning aler-dismissable fade show" role="aler">
        <%= error_msg %>
        <button type="button" class="close" data-dismiss="alert" aria-label="close"><span aria-hidden="true">&times;</span>
        </button>
        </div>
    <%}


<% if(error !=''){ %>
    <div class="alert alert-warning aler-dismissable fade show" role="aler">
        <%= error %>
        <button type="button" class="close" data-dismiss="alert" aria-label="close"><span aria-hidden="true">&times;</span>
        </button>
        </div>
    <%}) %>
<%}%>